<div class="container">
  <h3>Partials</h3>
  <table class="table table-bordered">
    <thead>
      <tr>
        <th>Name</th>
        <th>Actions</th>
      </tr>
    </thead>
    <% @partials.each do |partial| %>
      <tr>
        <td><%= link_to partial.name, partial %></td>

        <td>
          <%= link_to "Edit", edit_partial_path(partial) %> /
          <%= link_to "Delete", partial, method: :delete, confirm: "Are you sure?" %></td>
      <tr>
    <% end %>
  </table>
  <%= link_to 'New', new_partial_path %>
</div>
