<div class="container">
  <h3>Users</h3>

  <table class="table table-bordered">
    <thead>
      <tr>
        <th>User</th>
        <th>Organization</th>
        <th>Actions</th>
      </tr>
    </thead>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.email %></td>
        <td><%= user.organization.try(:name) %></td>
        <td><%= link_to "Edit", edit_admin_user_path(user) %></td>
      <tr>
    <% end %>
  </table>

  <%= link_to 'New', new_admin_user_path %>

</div>
